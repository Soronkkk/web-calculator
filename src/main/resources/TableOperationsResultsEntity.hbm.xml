<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="app.entities.TableOperationsResultsEntity" table="TABLE_OPERATIONS_RESULTS" schema="PUBLIC"
           catalog="CALCULATOR-DB">
        <id name="id">
            <column name="ID" sql-type="bigint(19)"/>
        </id>
        <property name="operation">
            <column name="OPERATION" sql-type="varchar(255)"/>
        </property>
        <property name="result">
            <column name="RESULT" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>