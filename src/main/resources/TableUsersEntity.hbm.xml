<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="app.entities.TableUsersEntity" table="TABLE_USERS" schema="PUBLIC" catalog="CALCULATOR-DB">
        <id name="id">
            <column name="ID" sql-type="bigint(19)"/>
        </id>
        <property name="name">
            <column name="NAME" sql-type="varchar(255)"/>
        </property>
        <property name="surname">
            <column name="SURNAME" sql-type="varchar(255)"/>
        </property>
        <property name="patronymic">
            <column name="PATRONYMIC" sql-type="varchar(255)"/>
        </property>
        <property name="username">
            <column name="USERNAME" sql-type="varchar(255)"/>
        </property>
        <property name="password">
            <column name="PASSWORD" sql-type="varchar(255)"/>
        </property>
        <property name="cookie">
            <column name="COOKIE" sql-type="uuid" not-null="true"/>
        </property>
        <many-to-one name="tableOperationsResultsByCookie" class="app.entities.TableOperationsResultsEntity">
            <column name="COOKIE" not-null="true"/>
        </many-to-one>
        <many-to-one name="tableOperationsResultsByCookie_0" class="app.entities.TableOperationsResultsEntity">
            <column name="COOKIE" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>